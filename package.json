{
  "name": "e-vrc",
  "version": "0.1.2",
  "private": true,
  "main": "public/electron.js",
  "homepage": ".",
  "dependencies": {
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "4.9.1",
    "classnames": "2.2.6",
    "electron-fetch": "1.7.1",
    "electron-is-dev": "1.2.0",
    "electron-unhandled": "3.0.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "7.2.1",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.3",
    "redux": "4.0.5",
    "redux-logger": "3.0.6",
    "redux-persist": "6.0.0",
    "redux-thunk": "2.3.0",
    "request": "2.88.2",
    "request-promise-native": "1.0.9",
    "tough-cookie": "4.0.0"
  },
  "resolutions": {
    "@types/react": "16.9.49"
  },
  "scripts": {
    "start": "cross-env EXTEND_ESLINT=true REACT_APP_STORE_LOG=true react-scripts start",
    "build": "cross-env EXTEND_ESLINT=true react-scripts build",
    "test": "react-scripts test --env=jsdom-fourteen --watchAll=false",
    "coverage": "react-scripts test --env=jsdom-fourteen --watchAll=false --coverage",
    "eject": "react-scripts eject",
    "upgrade": "yarn upgrade-interactive",
    "electron-dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "package": "yarn build && electron-builder build --win -c.extraMetadata.main=build/electron.js --publish never"
  },
  "build": {
    "appId": "com.example.electron-boilerplate",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/dom": "7.24.3",
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.0.4",
    "@testing-library/react-hooks": "3.4.2",
    "@testing-library/user-event": "12.1.6",
    "@types/classnames": "2.2.10",
    "@types/eslint-config-prettier": "^6",
    "@types/eslint-plugin-prettier": "^3",
    "@types/jest": "26.0.14",
    "@types/lodash.merge": "^4",
    "@types/node": "12.12.62",
    "@types/prettier": "^2",
    "@types/pretty": "^2",
    "@types/react": "16.9.49",
    "@types/react-dom": "16.9.8",
    "@types/react-redux": "7.1.9",
    "@types/react-router-dom": "5.1.5",
    "@types/redux-logger": "3.0.8",
    "concurrently": "5.3.0",
    "cross-env": "7.0.2",
    "electron": "10.1.2",
    "electron-builder": "22.8.1",
    "electron-packager": "15.1.0",
    "electron-renderer": "2.0.4",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-prettier": "3.1.4",
    "lodash.merge": "4.6.2",
    "node-sass": "4.14.1",
    "prettier": "2.1.2",
    "pretty": "2.0.0",
    "typescript": "3.9.7",
    "wait-on": "5.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
