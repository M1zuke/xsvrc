{
  "name": "xsvrc",
  "version": "0.4.2",
  "description": "Manage your VRC",
  "author": {
    "name": "Mizuke",
    "email": "...",
    "url": "https://github.com/M1zuke/xsvrc.git"
  },
  "private": true,
  "main": "public/electron.js",
  "homepage": ".",
  "repository": {
    "type": "git",
    "url": "https://github.com/M1zuke/xsvrc.git"
  },
  "dependencies": {
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "@mui/icons-material": "5.2.4",
    "@mui/material": "5.2.4",
    "classnames": "2.3.1",
    "electron-fetch": "1.7.4",
    "electron-is-dev": "2.0.0",
    "electron-log": "4.4.3",
    "electron-store": "8.0.1",
    "electron-unhandled": "3.0.2",
    "electron-updater": "4.6.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-json-view": "1.21.3",
    "react-redux": "7.2.6",
    "react-router-dom": "5.3.0",
    "react-scripts": "5.0.0",
    "redux": "4.1.2",
    "redux-logger": "3.0.6",
    "redux-persist": "6.0.0",
    "redux-thunk": "2.4.1",
    "request": "2.88.2",
    "request-promise-native": "1.0.9",
    "tough-cookie": "4.0.0",
    "ws": "8.3.0"
  },
  "resolutions": {
    "@types/react": "17.0.37"
  },
  "scripts": {
    "start": "cross-env EXTEND_ESLINT=true REACT_APP_STORE_LOG=true react-scripts start",
    "build": "cross-env EXTEND_ESLINT=true REACT_APP_STORE_LOG=false react-scripts build",
    "test": "react-scripts test --env=jsdom-fourteen --watchAll=false",
    "coverage": "react-scripts test --env=jsdom-fourteen --watchAll=false --coverage",
    "eject": "react-scripts eject",
    "upgrade": "yarn upgrade-interactive",
    "electron-dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "package": "yarn build && electron-builder build --win -c.extraMetadata.main=build/electron.js --publish never",
    "postinstall": "husky install",
    "update-yarn": "yarn set version latest && yarn plugin import interactive-tools && yarn plugin import typescript"
  },
  "build": {
    "appId": "com.xsvrc.app",
    "nsis": {
      "artifactName": "${productName}-Setup-${version}.${ext}",
      "oneClick": false,
      "perMachine": false,
      "deleteAppDataOnUninstall": true,
      "allowToChangeInstallationDirectory": true,
      "runAfterFinish": true,
      "differentialPackage": true
    },
    "directories": {
      "buildResources": "./build/"
    },
    "win": {
      "target": "nsis"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/dom": "8.11.1",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/react-hooks": "7.0.2",
    "@testing-library/user-event": "13.5.0",
    "@types/classnames": "2.3.1",
    "@types/electron-store": "3.2.0",
    "@types/jest": "27.0.3",
    "@types/lodash.merge": "4.6.6",
    "@types/node": "17.0.0",
    "@types/prettier": "2.4.2",
    "@types/pretty": "2.0.0",
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11",
    "@types/react-redux": "7.1.20",
    "@types/react-router-dom": "5.3.2",
    "@types/redux-logger": "3.0.9",
    "@types/request": "2.48.7",
    "@types/request-promise-native": "1.0.18",
    "@types/sass": "1.43.1",
    "@types/ws": "8.2.2",
    "concurrently": "6.5.0",
    "cross-env": "7.0.3",
    "electron": "16.0.5",
    "electron-builder": "22.14.10",
    "electron-packager": "15.4.0",
    "electron-renderer": "2.0.4",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "lint-staged": "12.1.2",
    "lodash.merge": "4.6.2",
    "prettier": "2.5.1",
    "pretty": "2.0.0",
    "pretty-quick": "3.1.2",
    "sass": "1.45.0",
    "typescript": "4.5.4",
    "wait-on": "6.0.0"
  },
  "packageManager": "yarn@3.1.0"
}
